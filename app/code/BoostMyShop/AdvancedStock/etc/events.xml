<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">

    <event name="catalog_product_save_after">
        <observer name="bms_advancedstock_catalog_product_save_after" instance="BoostMyShop\AdvancedStock\Observer\CatalogProductAfterSave"/>
    </event>

    <event name="advancedstock_stock_movement_save_after">
        <observer name="bms_advancedstock_advancedstock_stock_movement_save_after" instance="BoostMyShop\AdvancedStock\Observer\StockMovementAfterSave"/>
    </event>

    <event name="advancedstock_warehouse_item_available_quantity_after_change">
        <observer name="bms_advancedstock_advancedstock_warehouse_item_available_quantity_after_change" instance="BoostMyShop\AdvancedStock\Observer\WarehouseItemAvailableQuantityAfterChange"/>
    </event>

    <event name="sales_order_shipment_save_after">
        <observer name="bms_advancedstock_sales_order_shipment_save_after" instance="BoostMyShop\AdvancedStock\Observer\SalesOrderShipmentSaveAfter"/>
    </event>

    <event name="sales_order_item_save_after">
        <observer name="bms_advancedstock_sales_order_item_save_after" instance="BoostMyShop\AdvancedStock\Observer\SalesOrderItemSaveAfter"/>
    </event>

    <event name="advancedstock_order_item_warehouse_change">
        <observer name="bms_advancedstock_order_item_warehouse_change" instance="BoostMyShop\AdvancedStock\Observer\OrderItemWarehouseChange"/>
    </event>

    <event name="advancedstock_warehouse_item_reservation_to_process">
        <observer name="bms_advancedstock_warehouse_item_reservation_to_process" instance="BoostMyShop\AdvancedStock\Observer\OrderItemWarehouseReservationToProcess"/>
    </event>

    <event name="cataloginventory_stock_item_save_after">
        <observer name="bms_advancedstock_cataloginventory_stock_item_save_after" instance="BoostMyShop\AdvancedStock\Observer\CatalogInventoryStockItemAfterSave"/>
    </event>

    <event name="model_save_after">
        <observer name="bms_advancedstock_model_save_after" instance="BoostMyShop\AdvancedStock\Observer\ModelSaveAfter"/>
    </event>

    <event name="controller_action_predispatch_adminhtml_import_index">
        <observer name="bms_advancedstock_controller_action_predispatch_adminhtml_import_index" instance="BoostMyShop\AdvancedStock\Observer\ControllerActionPredispatchAdminhtmlImportIndex"/>
    </event>



</config>
