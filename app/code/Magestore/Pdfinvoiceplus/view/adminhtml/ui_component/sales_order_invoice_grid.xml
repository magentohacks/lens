<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction" class="Magestore\Pdfinvoiceplus\Ui\Component\MassAction">
            <action name="pdfinvoicesplus_print_invoice">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">pdfinvoicesplus_print_invoice</item>
                        <item name="label" xsi:type="string" translate="true">Print Invoices via PDF Invoice+</item>
                        <item name="url" xsi:type="url" path="pdfinvoiceplusadmin/printPdf/massPrintInvoice"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
